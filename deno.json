{
  "tasks": {
    "dev": "echo \"deno run --watch main.ts\"",
    "format": "deno fmt",
    "lint": "deno lint",
    "check": "deno check .",
    "executer": "deno --no-lock -A",
    "up:latest": "deno outdated --update --latest"
  },
  "imports": {},
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "strict": true
  }
}
